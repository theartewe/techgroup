<%- content_for :page_stylesheet do %>
  <%= stylesheet_link_tag 'portfolio', :media => 'screen' %>
<% end %>

<section class="portfolio-projects main">
  <%= render :partial => "project_header" %>
  <div class="content clear">
    <h2><%= t(:tech_group_projects_header) %></h2>
    
    <%= render "sidebar" %>
    <section class="projects-wrapper">
      <h3><%= t(:tech_group_projects_header) %></h3>
      <div id="projects">
        <%= render :partial => 'project', :collection => @objects %>
      </div>
    </section>
  </div>
</section>