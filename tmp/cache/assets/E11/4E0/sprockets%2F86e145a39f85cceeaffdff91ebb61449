FI"å(function() {
  var is_home, viewportmeta;

  if (navigator.userAgent.match(/iPhone/i) || navigator.userAgent.match(/iPad/i)) {
    viewportmeta = document.querySelector('meta[name="viewport"]');
    if (viewportmeta) {
      viewportmeta.content = 'width=device-width, minimum-scale=1.0, maximum-scale=1.0, initial-scale=1.0';
      document.body.addEventListener('gesturestart', (function() {
        return viewportmeta.content = 'width=device-width, minimum-scale=0.25, maximum-scale=1.6';
      }), false);
    }
  }

  is_home = function() {
    return $('section.home').length > 0;
  };

  $(function() {
    $(window).load(function() {
      if (is_home()) {
        $('#carousel').flexslider({
          controlNav: false
        });
      }
      return $('.slider').flexslider();
    });
    if (is_home()) {
      $(window).bind("load resize", function() {
        $(".equal").removeAttr("style");
        $(".equal, .slides > li, .flex-direction-nav, .banner").equalHeight();
        $("nav.services ul li a").removeAttr("style");
        return $("nav.services ul li a").equalHeight();
      });
      $('img.retina').retina();
    }
    return $('#contact_form').submit(function() {
      var params, url;
      event.stopPropagation();
      event.preventDefault();
      url = $(this).attr('action');
      params = $(this).serializeArray();
      return $.post(url, params, function(data) {
        if (data === "okay") return $('.thank-you').show();
      });
    });
  });

}).call(this);
